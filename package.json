{
  "name": "gourmetscrape",
  "productName": "gourmetscrape",
  "version": "1.2.0",
  "description": "Electron application for scraping",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc && chcp 65001",
    "package-win": "tsc && chcp 65001 && electron-packager . --asar=true --overwrite --platform=win32 --arch=ia32 --prune=true --icon=./assets/gourmet.ico --out=release-builds --electronVersion=28.2.0",
    "start": "tsc && chcp 65001 && electron ."
  },
  "keywords": [],
  "author": "ebs-uchimura <uchimura@ebisu-do.jp>",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^20.8.9",
    "csv-parse": "^5.5.5",
    "csv-stringify": "^6.4.6",
    "electron-log": "^5.1.0",
    "fs": "^0.0.1-security",
    "iconv-lite": "^0.6.3",
    "path": "^0.12.7",
    "puppeteer": "^22.4.1",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "electron": "^29.1.1",
    "electron-packager": "^17.1.2"
  }
}